int main() {
    int x;
    x = 2;

    if (x < 5) {
        print(x);
    } else {
        x = 10;
    }

    while (x < 12) {
        x = x + 1;
    }

    for (x = 0; x < 3; x = x + 1) {
        print(x);
    }

    switch (x) {
        case 1: print(x); break;
        case 2: print(x); break;
        default: print(x);
    }

    return 0;
}
